{
    "manifest_version": 3,
    "name": "Trackle (Web)",
    "version": "1.0",
    "description": "Monitor and control website permissions and track resource-intensive sites. The Chrome extension deployment of our mobile application Trackle.",
    "permissions": [
      "contentSettings",
      "tabs",
      "webRequest",
      "activeTab",
      "storage"
    ],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon19.png"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  